# AISDLC Environment Configuration

# LLM Integration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Aider Configuration
AIDER_MODEL=claude-3-5-sonnet-20241022
AIDER_EDITOR_MODEL=claude-3-haiku-20240307
AIDER_DOCKER_IMAGE=paulgauthier/aider
AIDER_TIMEOUT=300000

# Docker Configuration
DOCKER_HOST=unix:///var/run/docker.sock
DOCKER_MEMORY_LIMIT=2147483648
DOCKER_CPU_SHARES=1024

# Vector Database Configuration
VECTOR_DB_PATH=.aisdlc/vector.db
EMBEDDING_MODEL=all-MiniLM-L6-v2
EMBEDDING_DIMENSIONS=384

# GitHub Integration (for MCP tools)
GITHUB_TOKEN=your_github_token_here

# Development Configuration
NODE_ENV=development
LOG_LEVEL=info
DEBUG=false

# File System Configuration
WATCH_DEBOUNCE_MS=1000
MAX_FILE_SIZE_MB=10
