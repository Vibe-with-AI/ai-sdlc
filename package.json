{
	"name": "aishell",
	"description": "A new CLI generated with oclif",
	"version": "0.0.0",
	"author": "Parker Rex",
	"bin": {
		"aishell": "./bin/run.js"
	},
	"scripts": {
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"build": "bun build ./src/index.ts --outdir ./dist --target=node --format=esm",
		"format": "biome format --write ."
	},
	"dependencies": {
		"@oclif/core": "^4",
		"@oclif/plugin-help": "^6",
		"@oclif/plugin-plugins": "^5",
		"fs-extra": "^11.3.0",
		"js-yaml": "^4.1.0",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@oclif/test": "^4",
		"@types/fs-extra": "^11.0.4",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^18",
		"@types/uuid": "^10.0.0",
		"oclif": "^4",
		"shx": "^0.3.3",
		"ts-node": "^10",
		"typescript": "^5.8.3"
	},
	"packageManager": "bun@1.2.15",
	"files": ["./bin", "./dist", "./oclif.manifest.json"],
	"main": "dist/index.js",
	"type": "module",
	"oclif": {
		"bin": "aishell",
		"dirname": "aishell",
		"commands": "./dist/commands",
		"plugins": ["@oclif/plugin-help", "@oclif/plugin-plugins"],
		"topicSeparator": " ",
		"topics": {
			"hello": {
				"description": "Say hello to the world and others"
			}
		}
	}
}
